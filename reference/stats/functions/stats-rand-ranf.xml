<refentry xml:id="function.stats-rand-ranf" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>stats_rand_ranf</refname>
  <refpurpose>Retourne un nombre flottant aléatoire entre 0 et 1</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>float</type><methodname>stats_rand_ranf</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retourne un &float; aléatoire à partir d'une loi uniforme
   comprise entre 0 (exclusif) et 1 (exclusif).
  </para>
 </refsect1>

 <!--
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
  </para>
 </refsect1>
 -->

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Un &float; aléatoire.
  </para>
 </refsect1>


</refentry>
