<refentry xml:id="function.mysqli-get-cache-stats" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>mysqli_get_cache_stats</refname>
  <refpurpose>Retourne les statistiques sur le cache du client Zval</refpurpose>
 </refnamediv>

 <refsynopsisdiv>
  <warning xmlns="http://docbook.org/ns/docbook">
   <simpara>Cette fonction a été <emphasis>SUPPRIMÉE</emphasis> à partir de PHP 5.4.0.</simpara>
  </warning>
 </refsynopsisdiv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>mysqli_get_cache_stats</methodname>
   <void/>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Retourne &array; vide.
   &mysqli.available.mysqlnd;
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un &array; vide en cas de succès, &false; sinon.
  </para>
 </refsect1>

 <refsect1 role="changelog">
  &reftitle.changelog;
  <para>
   <informaltable>
    <tgroup cols="2">
     <thead>
      <row>
       <entry>&Version;</entry>
       <entry>&Description;</entry>
      </row>
     </thead>
     <tbody>
      <row>
       <entry>5.4.0</entry>
       <entry>
        <function>mysqli_get_cache_stats</function> a été supprimée.
       </entry>
      </row>
      <row>
       <entry>5.3.0</entry>
       <entry>
        <function>mysqli_get_cache_stats</function> a été ajouté en tant que stub.
       </entry>
      </row>
     </tbody>
    </tgroup>
   </informaltable>
  </para>
 </refsect1>

</refentry>

